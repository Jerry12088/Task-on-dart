// Task 1
int addTwo(int a, int b) {
  return a + b;
}

// Task 2  
int subtractTwo(int a, int b) {
  return a - b;  
}

// Task 3
int multiplyTwo(int a, int b) {
  return a * b;
}

// Task 4
double divideTwo(int a, int b) {
  return a / b;
}

// Task 5
int stringLength(String text) {
  return text.length;
}

// Task 6
dynamic getFirstElement(List list) {
  return list[0]; 
}
